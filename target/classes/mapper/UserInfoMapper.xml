<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.UserInfoDao">



    <insert id="insert" parameterType="entity.UserInfo">

        insert into USER_INFO(USER_ID,USER_NAME,USER_PWD,USER_SCORE,USER_WINNUM,USER_LOSENUM)

        values
        (#{userID},#{userName},#{userPWD},#{userScore},#{userWinNum},#{userLoseNum})
    </insert>

    <insert id="insertByMap" parameterType="map">

        insert into USER_INFO
        <foreach collection="" open="(" close=")" separator="," index="index" item="item">#{index}</foreach>
        values
        <foreach collection="userInfo" open="(" close=")" separator="," index="index" item="item">#{item}</foreach>
    </insert>
</mapper>